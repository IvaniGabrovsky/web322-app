<div class="row">
  <div class="col-md-12">
    <h2>WEB322 {{userName}} ( {{email}} ) History</h2>
    <br />
    {{#if errorMessage}}
      <div class="alert alert-danger alert-dismissible fade in" role="alert">
        <button
          type="button"
          class="close"
          data-dismiss="alert"
          aria-label="Close"
        >
          <span aria-hidden="true">Ã—</span>
        </button>
        <strong>Error:</strong>
        {{errorMessage}}
      </div>
    {{/if}}
    <table class="table">
      <thead>
        <tr>
          <th>Login Date/Time</th>
          <th>Client Information</th>
        </tr>
      </thead>
      <tbody>
        {{#each userHistory}}
          <tr>
            <td>{{this.dateTime}}</td>
            <td>{{this.userAgent}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
    <hr />
  </div>
</div>